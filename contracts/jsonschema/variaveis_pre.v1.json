{
  "$schema":"https://json-schema.org/draft/2020-12/schema",
  "title":"VariaveisPreV1",
  "type":"object",
  "required":[
    "total_operacoes","periodo","contratos_media","contratos_max","contratos_soma",
    "resultado_liquido_acumulado","resultado_bruto","resultado_acumulado_pontos","taxas",
    "percentual_liquido","percentual_taxas","percentual_restante",
    "parametros_ativos","resultado_liquido_media"
  ],
  "properties":{
    "total_operacoes":{"type":"integer","minimum":0},
    "periodo":{"type":"string"},
    "contratos_media":{"type":"number"},
    "contratos_max":{"type":"number"},
    "contratos_soma":{"type":"number"},
    "resultado_liquido_acumulado":{"type":"number"},
    "resultado_bruto":{"type":"number"},
    "resultado_acumulado_pontos":{"type":"number"},
    "taxas":{"type":"number"},
    "percentual_liquido":{"type":"number"},
    "percentual_taxas":{"type":"number"},
    "percentual_restante":{"type":"number"},
    "parametros_ativos":{
      "type":"object",
      "required":["multiplicador","taxa","deslocamento_min","valor_por_ponto","contratos"],
      "properties":{
        "multiplicador":{"type":"number"},
        "taxa":{"type":"number"},
        "deslocamento_min":{"type":"number"},
        "valor_por_ponto":{"type":"number"},
        "contratos":{"type":"integer"}
      },
      "additionalProperties":false
    },
    "resultado_liquido_media":{"type":"number"}
  },
  "additionalProperties":false
}
